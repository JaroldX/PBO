package com.mycompany.calculator;

class Kubus extends Nilai {
    private double sisi;

    public Kubus(int sisi) {
        this.sisi = sisi;
    }

    @Override
    double Volume() {
        return sisi * sisi * sisi;
    }

    @Override
    double LuasPermukaan() {
        return 6 * sisi * sisi;
    }
}
